  "dependencies": {
    "yargs": "^15.3.1",
    "figlet": "^1.3.0",
    "ora": "^4.0.3"
  },
  "devDependencies": {
    "@types/ora": "^3.2.0",
    "@types/yargs": "^15.0.4",
